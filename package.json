{
  "name": "react-graphql-ssr-boilerplate",
  "version": "1.0.0",
  "description": "React Graphql Express isomorphic rendering boilerplate.",
  "main": "src/server.js",
  "scripts": {
    "babel-node": "babel-node --presets=env",
    "build:client": "webpack --config webpack.dev.babel.js",
    "start:ssr": "npm run build:client && nodemon --exec npm run babel-node -- ./src/server.js",
    "start": "nodemon --exec npm run babel-node -- ./src/server.js",
    "dev-server": "webpack-dev-server --open --config webpack.dev.babel.js",
    "server": "npm run babel-node -- ./src/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "React",
    "Redux",
    "React-Router",
    "GraphQL",
    "Express",
    "Webpack",
    "Isomorphic-Rendering"
  ],
  "author": "Sumit Kumar",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "html-webpack-plugin": "^3.2.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.7",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
  }
}
